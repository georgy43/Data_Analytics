   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv('retail_sales_dataset.csv')\n",
    "print(f'Dataset shape: {df.shape}')\n",
    "df.head()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 3. Data Cleaning & Preprocessing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Check data types and missing values\n",
    "print('=== Data Types ===')\n",
    "print(df.dtypes)\n",
    "print('\\n=== Missing Values ===')\n",
    "print(df.isnull().sum())\n",
    "print('\\n=== Duplicate Rows ===')\n",
    "print(df.duplicated().sum())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert Date column to datetime\n",
    "df['Date'] = pd.to_datetime(df['Date'])\n",
    "\n",
    "# Extract useful time features\n",
    "df['Month'] = df['Date'].dt.month\n",
    "df['Month_Name'] = df['Date'].dt.strftime('%b')\n",
    "df['Day_of_Week'] = df['Date'].dt.day_name()\n",
    "df['Quarter'] = df['Date'].dt.quarter\n",
    "df['Year'] = df['Date'].dt.year\n",
    "\n",
    "print('‚úÖ Date features extracted!')\n",
    "df[['Date','Month','Month_Name','Day_of_Week','Quarter']].head()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 4. Descriptive Statistics"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print('=== Descriptive Statistics ===')\n",
    "df[['Age','Quantity','Price per Unit','Total Amount']].describe().round(2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Key business metrics\n",
    "print(f\"üí∞ Total Revenue        : ‚Çπ{df['Total Amount'].sum():,.0f}\")\n",
    "print(f\"üì¶ Total Transactions   : {len(df):,}\")\n",
    "print(f\"üë§ Unique Customers     : {df['Customer ID'].nunique():,}\")\n",
    "print(f\"üõçÔ∏è  Avg Transaction Value: ‚Çπ{df['Total Amount'].mean():,.2f}\")\n",
    "print(f\"üìä Median Order Value   : ‚Çπ{df['Total Amount'].median():,.2f}\")\n",
    "print(f\"üîº Max Single Purchase  : ‚Çπ{df['Total Amount'].max():,}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 5. Time Series Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Monthly sales trend\n",
    "monthly_sales = df.groupby(['Year','Month','Month_Name'])['Total Amount'].sum().reset_index()\n",
    "monthly_sales = monthly_sales.sort_values(['Year','Month'])\n",
    "monthly_sales['Period'] = monthly_sales['Month_Name'] + ' ' + monthly_sales['Year'].astype(str)\n",
    "\n",
    "plt.figure(figsize=(14,5))\n",
    "plt.plot(monthly_sales['Period'], monthly_sales['Total Amount'], marker='o', color='steelblue', linewidth=2)\n",
    "plt.fill_between(monthly_sales['Period'], monthly_sales['Total Amount'], alpha=0.15, color='steelblue')\n",
    "plt.xticks(rotation=45, ha='right')\n",
    "plt.title('üìà Monthly Sales Revenue Trend', fontsize=14, fontweight='bold')\n",
    "plt.xlabel('Month')\n",
    "plt.ylabel('Total Revenue')\n",
    "plt.tight_layout()\n",
    "plt.savefig('monthly_sales_trend.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Sales by day of week\n",
    "day_order = ['Monday','Tuesday','Wednesday','Thursday','Friday','Saturday','Sunday']\n",
    "day_sales = df.groupby('Day_of_Week')['Total Amount'].sum().reindex(day_order)\n",
    "\n",
    "plt.figure(figsize=(10,5))\n",
    "bars = plt.bar(day_sales.index, day_sales.values, color=sns.color_palette('Blues_d', 7))\n",
    "plt.title('üìÖ Total Sales by Day of Week', fontsize=14, fontweight='bold')\n",
    "plt.xlabel('Day')\n",
    "plt.ylabel('Total Revenue')\n",
    "for bar in bars:\n",
    "    plt.text(bar.get_x() + bar.get_width()/2, bar.get_height() + 200,\n",
    "             f'{bar.get_height():,.0f}', ha='center', va='bottom', fontsize=9)\n",
    "plt.tight_layout()\n",
    "plt.savefig('sales_by_day.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 6. Customer & Product Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Sales by product category\n",
    "cat_sales = df.groupby('Product Category')['Total Amount'].sum().sort_values(ascending=False)\n",
    "\n",
    "fig, axes = plt.subplots(1, 2, figsize=(14, 5))\n",
    "\n",
    "# Bar chart\n",
    "axes[0].bar(cat_sales.index, cat_sales.values, color=sns.color_palette('Set2', len(cat_sales)))\n",
    "axes[0].set_title('Revenue by Product Category', fontweight='bold')\n",
    "axes[0].set_xlabel('Category')\n",
    "axes[0].set_ylabel('Total Revenue')\n",
    "\n",
    "# Pie chart\n",
    "axes[1].pie(cat_sales.values, labels=cat_sales.index, autopct='%1.1f%%',\n",
    "            colors=sns.color_palette('Set2', len(cat_sales)), startangle=90)\n",
    "axes[1].set_title('Revenue Share by Category', fontweight='bold')\n",
    "\n",
    "plt.suptitle('üõçÔ∏è Product Category Analysis', fontsize=14, fontweight='bold')\n",
    "plt.tight_layout()\n",
    "plt.savefig('category_analysis.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Gender analysis\n",
    "fig, axes = plt.subplots(1, 2, figsize=(14, 5))\n",
    "\n",
    "gender_rev = df.groupby('Gender')['Total Amount'].sum()\n",
    "axes[0].pie(gender_rev, labels=gender_rev.index, autopct='%1.1f%%',\n",
    "            colors=['#FF9999','#66B2FF'], startangle=90)\n",
    "axes[0].set_title('Revenue Split by Gender', fontweight='bold')\n",
    "\n",
    "gender_cat = df.groupby(['Gender','Product Category'])['Total Amount'].sum().unstack()\n",
    "gender_cat.plot(kind='bar', ax=axes[1], colormap='Set2')\n",
    "axes[1].set_title('Category Preferences by Gender', fontweight='bold')\n",
    "axes[1].set_xlabel('Gender')\n",
    "axes[1].set_ylabel('Total Revenue')\n",
    "axes[1].tick_params(axis='x', rotation=0)\n",
    "axes[1].legend(title='Category')\n",
    "\n",
    "plt.suptitle('üë• Gender-Based Customer Analysis', fontsize=14, fontweight='bold')\n",
    "plt.tight_layout()\n",
    "plt.savefig('gender_analysis.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Age distribution\n",
    "df['Age Group'] = pd.cut(df['Age'], bins=[17,25,35,45,55,65], labels=['18-25','26-35','36-45','46-55','56-65'])\n",
    "\n",
    "fig, axes = plt.subplots(1, 2, figsize=(14, 5))\n",
    "\n",
    "sns.histplot(df['Age'], bins=20, kde=True, color='steelblue', ax=axes[0])\n",
    "axes[0].set_title('Age Distribution of Customers', fontweight='bold')\n",
    "axes[0].set_xlabel('Age')\n",
    "\n",
    "age_revenue = df.groupby('Age Group', observed=True)['Total Amount'].sum()\n",
    "axes[1].bar(age_revenue.index.astype(str), age_revenue.values,\n",
    "            color=sns.color_palette('Oranges_d', len(age_revenue)))\n",
    "axes[1].set_title('Revenue by Age Group', fontweight='bold')\n",
    "axes[1].set_xlabel('Age Group')\n",
    "axes[1].set_ylabel('Total Revenue')\n",
    "\n",
    "plt.suptitle('üéÇ Age-Based Customer Analysis', fontsize=14, fontweight='bold')\n",
    "plt.tight_layout()\n",
    "plt.savefig('age_analysis.png', dpi=150, bbox_inches='tight')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
